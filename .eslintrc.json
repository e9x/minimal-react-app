{
	"extends": ["eslint:recommended", "react-app", "plugin:jsx-a11y/recommended"],
	"rules": {
		"@typescript-eslint/consistent-type-imports": "warn",
		"react/jsx-no-undef": ["error", { "allowGlobals": true }]
	},
	"globals": {
		"React": "readonly",
		"ReactDOM": "readonly",
		"ReactDOMClient": "readonly",
		"ReactHelmet": "readonly"
	}
}
